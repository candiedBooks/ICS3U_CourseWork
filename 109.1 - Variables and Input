import java.io.*;
import java.util.*;

public class Main {
  public static void main(String[] args) {
    // a)
int i = -47, j = 35; 
//output first line using i and j
// b)
boolean done = false; 
//output the 2nd line of output
// c)
double x = 0.012, y = 2.7374e1; 
//use one line to output the 3rd and 4th line

    System.out.println(" The value of i is " + i + " while the value of j is "+ j +". \n We are not "+ done +" yet. \n x -> "+ x +" \n y -> "+ y +" \n");
  }
}
