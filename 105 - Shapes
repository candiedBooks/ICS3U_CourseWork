import java.io.*;
import java.util.*;

public class Main {
  public static void main(String[] args) {
    //parallelogram
    System.out.println("PARALLELOG \n A        R \n  R        A \n   ALLELOGRAM \n");
    //trapezoid
    System.out.println("   APEZO\n  R     I\n TRAPEZOID \n");
    //rectangle
    System.out.println("RECTANGLE\nE       L\nC       G\nT       N\nA       A\nN       T\nG       C\nL       E\nELGNATCER\n");
    //diamond (although this shape I think is a rhombus)
    System.out.println("   D\n  III\n AAAAA\nMMMMMMM\n OOOOO\n  NNN\n   D\n");
  }
}
